
{
    "listen_addr": "0.0.0.0",
    "listen_port": 5445,
    "static_resources": "resources",
    "sqlite3_database": "~/.webdisk/wd.db",
    "allow_http_redirect": true,

    "filesystem": {
        "type": "multi",
        "data": [
            {
                "srcPrefix": "/noexists",
                "dstPrefix": "/",
                "config": {
                    "type": "alioss",
                    "data": {
                        "region": "",
                        "accessKeyId": "",
                        "accessKeySecret": "",
                        "bucket": ""
                    }
                }
            },
            {
                "srcPrefix": "/",
                "dstPrefix": "/",
                "config": {
                    "type": "local"
                }
            }
        ]
    }
}

